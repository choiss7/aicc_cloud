# AWS Connect 콜센터 프로젝트 환경 변수 설정

# AWS 기본 설정
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here

# AWS Connect 설정
AWS_CONNECT_INSTANCE_ID=your_connect_instance_id
AWS_CONNECT_CONTACT_FLOW_ID=your_contact_flow_id
AWS_CONNECT_QUEUE_ID=your_queue_id

# AWS Lex 설정
AWS_LEX_BOT_NAME=your_lex_bot_name
AWS_LEX_BOT_ALIAS=your_lex_bot_alias
AWS_LEX_BOT_VERSION=your_lex_bot_version

# AWS Lambda 설정
AWS_LAMBDA_FUNCTION_NAME=your_lambda_function_name

# AWS S3 설정
AWS_S3_BUCKET_NAME=your_s3_bucket_name
AWS_S3_REGION=ap-northeast-2

# AWS DynamoDB 설정
AWS_DYNAMODB_TABLE_NAME=your_dynamodb_table_name
AWS_DYNAMODB_REGION=ap-northeast-2

# AWS DynamoDB 설정
AWS_DYNAMODB_TABLE_NAME=your_dynamodb_table_name
AWS_DYNAMODB_REGION=ap-northeast-2

# AWS CloudWatch 설정
AWS_CLOUDWATCH_LOG_GROUP=/aws/lambda/connect-chatbot
AWS_CLOUDWATCH_LOG_STREAM=chatbot-logs

# 애플리케이션 설정
APP_NAME=AWS Connect 콜센터
APP_VERSION=0.5
APP_DEBUG=false
APP_LOG_LEVEL=INFO

# API 설정
API_HOST=0.0.0.0
API_PORT=8000
API_SECRET_KEY=your_secret_key_for_api

# 보안 설정
JWT_SECRET_KEY=your_jwt_secret_key
JWT_ALGORITHM=HS256
JWT_EXPIRATION_HOURS=24

# 로깅 설정
LOG_FILE_PATH=logs/app.log
LOG_MAX_SIZE=10MB
LOG_BACKUP_COUNT=5
