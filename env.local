# 로컬 개발환경 설정
ENVIRONMENT=development
APP_DEBUG=true
APP_LOG_LEVEL=DEBUG

# API 서버 설정
API_HOST=127.0.0.1
API_PORT=8000
API_SECRET_KEY=local-dev-secret-key-for-testing
JWT_SECRET_KEY=local-dev-jwt-secret-key-for-testing

# AWS 설정 (로컬 테스트용 - 실제 값으로 변경 필요)
AWS_REGION=ap-northeast-2
AWS_ACCESS_KEY_ID=test-access-key
AWS_SECRET_ACCESS_KEY=test-secret-key

# AWS Connect 설정 (테스트용)
AWS_CONNECT_INSTANCE_ID=test-instance-id
AWS_CONNECT_CONTACT_FLOW_ID=test-flow-id
AWS_CONNECT_QUEUE_ID=test-queue-id

# DynamoDB 설정 (로컬 테스트용)
AWS_DYNAMODB_TABLE_NAME=aicc-conversations-local
DYNAMODB_TABLE_NAME=aicc-conversations-local

# S3 설정 (로컬 테스트용)
AWS_S3_BUCKET_NAME=aicc-test-bucket
S3_BUCKET_NAME=aicc-test-bucket

# Bedrock 설정
BEDROCK_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0
BEDROCK_REGION=us-east-1

# 테스트 설정
TESTING=true
TEST_DATABASE_URL=sqlite:///test.db 